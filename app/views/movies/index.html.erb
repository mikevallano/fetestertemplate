<h1>All Movies</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td><%= movie.id %></td>
        <td><%= movie.title %></td>
        <td><%= link_to 'View', movie %></td>
      </tr>
    <% end %>
  </tbody>
</table>
